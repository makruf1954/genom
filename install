Â  Â  tput civis

Â  Â  echo -ne "Â  \033[0;33mUpdate Domain.. \033[1;37m- \033[0;33m["

Â  Â  while true; do

Â  Â  Â  Â  for ((i = 0; i < 18; i++)); do

Â  Â  Â  Â  Â  Â  echo -ne "\033[0;32m#"

Â  Â  Â  Â  Â  Â  sleep 0.1s

Â  Â  Â  Â  done

Â  Â  Â  Â  [[ -e $HOME/fim ]] && rm $HOME/fim && break

